    "koa-bodyparser": "^4.3.0",
    "koa-router": "^9.0.1",
    "koa-static": "^5.0.0",
    "nodemon": "^2.0.4",
    "onezip": "^4.2.0",
    "parcel-bundler": "^1.12.4",
